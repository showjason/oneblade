# OneBlade SRE Agent 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 应用环境: dev, prod
APP_ENV=dev

# OpenAI 配置
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4

# 数据库配置
DATABASE_URL=postgres://user:pass@localhost:5432/oneblade?sslmode=disable

# Redis 配置
REDIS_ADDR=localhost:6379

# Prometheus 配置
PROMETHEUS_URL=http://localhost:9090

# PagerDuty 配置
PAGERDUTY_API_KEY=your-pagerduty-api-key

# OpenSearch 配置
OPENSEARCH_URL=http://localhost:9200
OPENSEARCH_USER=admin
OPENSEARCH_PASS=admin
